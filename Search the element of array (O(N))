#include <iostream>

using namespace std;

const int n=10^7+10;
int hsh[n];


int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        hsh[a[i]++];
        
    }
    
    
    int t;
    cin>>t;
    
    while(t--)
    {
        int flag=0;
        int e;
        cin>>e;
       
        
        cout<<hsh[e]<<endl;
    }
}

