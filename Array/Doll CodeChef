#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
      int count=0;
      int n,p;
      cin>>n;
      cin>>p;
      int a[n];
      for(int i=0;i<n;i++){
          cin>>a[i];
      }
     
   
      
      for(int i=0;i<n;i++)
      {
          if(a[i]>p)
          {
              count++;
          }
      }
      
      cout<<count<<endl;
      
    }
}
