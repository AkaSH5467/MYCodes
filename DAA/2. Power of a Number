/****************************************************************** Naive Approach ***********************************************************************/

#include <iostream>

using namespace std;
int powerf(int a , int b)
{
    int res = 1;
    
    for(int i=1;i<=b;i++)
    {
        res = res*a;
    }
    
    return res;
}


int main()
{
    int n,m;
    cin>>n>>m;
    
    
    cout<<powerf(n,m)<<endl;

    return 0;
}

