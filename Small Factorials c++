#include <iostream>

using namespace std;
int factorial(int m)
{
    if(m>0)
    return m*factorial(m-1);
    
    else
    return 1;
    
}



int main()
{
    int n,a;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int S;
        cin>>a;
        S=factorial(a);
        cout<<S;
    }

    return 0;
}
