/*this question asks us to print the string in different lines and we have to capitalize it*/

#include <bits/stdc++.h>

using namespace std;
int upper(char S)
{
    return 'A'+ (S-'a');
}
int main()
{
    while(true)
    {
         string st;
         cin>>st;
         
         if(st.size()==0)
         {
             break;
         }
         
         for(int i=0;i<st.size();++i)
         {
            st[i]=upper(st[i]);
         }
        cout<<st<<endl; 
        
    }
   

    return 0;
}

/*
Input 
my name is akash

Output
MY
NAME 
IS 
AKASH

*/
