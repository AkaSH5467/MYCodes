#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        
    }
    
    int t;
    cin>>t;
    
    while(t--)
    {
        int flag=0;
        int e;
        cin>>e;
        for(int i=0;i<n;i++)
        {
            if(a[i]==e)
            {
                flag++;
            }
        }
        
        cout<<flag<<endl;
    }
}
