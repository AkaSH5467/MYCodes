
#include <iostream>

using namespace std;

int main()
{ 
    int n;
    int a,b,c;
    cin>>n;
    for(int i=0;i<n;i++)
        {
            cin>>a;
             int sm=0;
        
            while(a!=0)
            
            {  b=a%10;           //main
               
               a=a/10;
               sm=sm*10+b;        //logic  /* the point where the mistake was there...kindly memorize it*/
            }
            cout<<sm<<endl;
           
        }
        

    return 0;
}
