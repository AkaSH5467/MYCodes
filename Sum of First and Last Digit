#include <iostream>

using namespace std;

int main()
{ 
    int n;
    int a,b,c;
    cin>>n;
    for(int i=0;i<n;i++)
        {
            cin>>a;
            b= a%10;
            while(a>=10)
            
            {
                a=a/10;
            }
            
            int c=a;
            int k=0;
            k=c+b;
            cout<<k<<endl;
        }
        

    return 0;
}
