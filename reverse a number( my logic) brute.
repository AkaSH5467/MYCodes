#include <iostream>

using namespace std;

int main()
{ 
    int n;
    int a,b,c;
    cin>>n;
    for(int i=0;i<n;i++)
        {
            cin>>a;
          
            while(a!=0)
            
            {  b=a%10;           
               cout<<b;
               a=a/10; 
            }
            
           
        }
        

    return 0;
}
