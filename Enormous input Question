#include<bits/stdc++.h>

    using namespace std;

    int main()

    {
        int n,k,a;
        cin>>n>>k;
        int count=0;  //silly mistake
        while(n--)  //learned something new
        {
            cin>>a;
            if(a%k==0) //main logic
            {
                count++;
            }
        }
        cout<<count<<endl;
       return 0;
    }
